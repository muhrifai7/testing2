{
  "name": "typescript-getting-started",
  "version": "1.0.0",
  "description": "A sample TypeScript app using Express",
  "main": "dist/index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --config ./dist/typeorm/config/ormconfig.js",
    "typeorm:migration:generate": "yarn typeorm -- migration:generate -n Migrations -p",
    "typeorm:migration:run": "yarn typeorm -- migration:run",
    "start": "node .",
    "build": "tsc",
    "dev": "nodemon --watch src -e ts --exec ts-node src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/ejs": "^3.0.5",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.22",
    "nodemon": "^2.0.7",
    "typescript": "^4.1.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/dateformat": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/validator": "^13.7.0",
    "ttypescript": "^1.5.13",
    "typescript-transform-paths": "^3.3.1"
  },
  "dependencies": {
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "pg": "^8.7.1",
    "typeorm": "^0.2.41",
    "typeorm-naming-strategies": "^2.0.0",
    "validator": "^13.7.0",
    "ts-node": "^10.4.0"
  },
  "nodemonConfig": {
    "ext": "ts",
    "exec": "npm run build && npm start"
  }
}
